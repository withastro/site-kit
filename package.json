{
  "name": "@astrojs/site-kit",
  "version": "0.0.0",
  "description": "Styles, components, and tooling config for use in *.astro.build websites",
  "type": "module",
  "author": "withastro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withastro/site-kit.git"
  },
  "bugs": "https://github.com/withastro/site-kit/issues",
  "homepage": "https://github.com/withastro/site-kit#readme",
  "exports": {
    "./assets/*": "./assets/*",
    "./components/*": "./components/*",
    "./icons/*": "./icons/*",
    "./eslint": "./.eslintrc.cjs",
    "./prettier": "./.prettierrc.cjs",
    "./typescript": "./tsconfig.json"
  },
  "files": [
    "assets",
    "components",
    "icons",
    "layouts",
    ".eslintrc.js",
    ".prettierrc.cjs",
    "tsconfig.json"
  ],
  "scripts": {
    "format": "run-s format:*",
    "format:prettier": "prettier -w --plugin-search-dir=. .",
    "format:eslint": "eslint --fix \"**/*.astro\"",
    "lint": "run-s format:*",
    "lint:prettier": "prettier --plugin-search-dir=. .",
    "lint:eslint": "eslint \"**/*.astro\""
  },
  "dependencies": {
    "@astrojs/image": "^0.12.1",
    "astro": "^1.6.15",
    "smartypants": "^0.1.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-plugin-astro": "^0.21.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prettier-plugin-astro": "^0.7.0"
  },
  "packageManager": "pnpm@7.12.2"
}
